{
    "version": 2,
    "headers": [
        {
            "source": "/ (.*)",
            "headers" : [
                {
                    "key" : "Content-Security-Policy",
                    "value" : "default-src 'self' ; img-sre 'self' data:; script-sre 'self' 'unsafe-eval';stvle-src 'self' 'unsafe-inline'"
                },
                {
                    "key" : "Referrer-Policy",
                    "value" : "no-referrer-when-downgrade"
                },
                {
                    "key" : "X-Content-Type-Options",
                    "value" : "nosniff"
                },
                {
                    "key" : "X-Frame-Options",
                    "value" : "DENY"
                },
                {
                    "key" : "X-XSS-Protection",
                    "value" : "1; mode=block"
                },
                {
                    "key" : "X-Robots-Tag",
                    "value" : "noindex"
                }
            ]
        }    
    ]
}
